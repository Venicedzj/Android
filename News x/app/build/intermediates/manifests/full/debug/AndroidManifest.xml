<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhaungjie.news"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/news"
        android:label="NodeNews"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.zhaungjie.news.conctroller.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zhaungjie.news.conctroller.MainActivity" />
        <activity android:name="com.example.zhaungjie.news.view.NewsInfoActivity" />
        <activity android:name="com.example.zhaungjie.news.conctroller.SignUpActivity" />
    </application>

</manifest>