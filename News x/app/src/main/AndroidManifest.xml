<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhaungjie.news">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/news"
        android:label="NodeNews"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".conctroller.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".conctroller.MainActivity"/>
        <activity android:name=".view.NewsInfoActivity" />
        <activity android:name=".conctroller.SignUpActivity"/>
    </application>

</manifest>